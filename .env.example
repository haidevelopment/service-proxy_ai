# Server Configuration
PORT=3000
NODE_ENV=dev

DB_HOST=127.0.0.1
DB_PORT=3306
DB_USER=root
DB_PASSWORD=
DB_NAME=partner

# Security Configuration
# API Key for client authentication (generate a strong random key)
API_KEY=your_secure_api_key_here_min_32_chars

# Whitelist IPs are loaded from MySQL table `setting` (db=partner) with group=WHITELIST_IP_SETTING_GROUP
WHITELIST_CACHE_TTL_MS=60000

DB_TYPE=mysql
DB_SETTING_TYPE=partner
OPEN_AI_PROMPT_SETTING_GROUP=prompt
WHITELIST_IP_SETTING_GROUP=whiteIP
GEMINI_SETTING_GROUP=gemini
SPEECH_SETTING_GROUP=speech