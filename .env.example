# Server Configuration
PORT=3000
NODE_ENV=dev

DB_HOST=127.0.0.1
DB_PORT=3306
DB_USER=root
DB_PASSWORD=
DB_NAME=partner

# Gemini AI API Key
GEMINI_API_KEY=your_gemini_api_key_here

# Security Configuration
# API Key for client authentication (generate a strong random key)
API_KEY=your_secure_api_key_here_min_32_chars

# IP Whitelist
# Whitelist IPs are loaded from MySQL table `setting` (db=partner) with type=WHITELIST_IP_SETTING_TYPE
WHITELIST_CACHE_TTL_MS=60000

DB_TYPE=mysql
DB_SETTING_TYPE=partner
OPEN_AI_PROMPT_SETTING_TYPE=10
WHITELIST_IP_SETTING_TYPE=8
SPEECH_AI_SETTING_TYPE=7